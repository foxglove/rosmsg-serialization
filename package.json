{
  "name": "@foxglove/rosmsg-serialization",
  "version": "1.0.1",
  "description": "ROS1 (Robot Operating System) message serialization, for reading and writing bags and network messages",
  "license": "MIT",
  "keywords": [
    "ros",
    "ros1",
    "robot",
    "operating",
    "system",
    "serialization",
    "protocol",
    "rosbag",
    "serde"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/foxglove/rosmsg-serialization.git"
  },
  "author": {
    "name": "Foxglove Technologies Inc",
    "email": "contact@foxglove.dev"
  },
  "homepage": "https://github.com/foxglove/rosmsg-serialization",
  "module": "dist/esm/index.js",
  "main": "dist/cjs/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:esm": "tsc -b tsconfig.json",
    "build:cjs": "tsc -b tsconfig.cjs.json",
    "prepack": "yarn build",
    "lint:ci": "eslint --report-unused-disable-directives .",
    "lint": "eslint --report-unused-disable-directives --fix .",
    "prepublishOnly": "yarn lint:ci && yarn test",
    "test": "jest",
    "bench": "ts-node --project tsconfig.cjs.json bench/index.ts"
  },
  "engines": {
    "node": ">= 14"
  },
  "dependencies": {
    "@foxglove/rosmsg": "^1.0.0"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "0.13.0",
    "@foxglove/tsconfig": "^1.0.0",
    "@types/eslint": "^7",
    "@types/eslint-plugin-prettier": "^3",
    "@types/jest": "^26.0.23",
    "@types/prettier": "2.3.2",
    "@typescript-eslint/eslint-plugin": "4.28.4",
    "@typescript-eslint/parser": "4.28.4",
    "console-table-printer": "^2.9.0",
    "esbuild": "0.12.15",
    "esbuild-jest": "0.5.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.4.0",
    "eslint-plugin-prettier": "3.4.0",
    "jest": "27.0.6",
    "kelonio": "^0.6.0",
    "percentile": "^1.4.0",
    "prettier": "2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "4.3.5"
  }
}
